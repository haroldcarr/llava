(define (tc2 x)
 (list 'R1
	(call/cc
         (lambda (R1)
          (list 'R2
		 (call/cc
                  (lambda (R2)
                   (list 'R3
			  (call/cc
                           (lambda (R3)
			     (tcaux R1 R2 R3 x)))))))))
	x))

(define (tcaux r1 r2 r3 x)
  (if (null? x) (r1 null) null)
  (let ((cx (car x)))
    (cond ((equal? cx 1)
	    (r1 x))
	   ((equal? cx 2)
	    (r2 x))
	   ((equal? cx 3)
	    (r3 x))
	   (else
	    (dprintln (car x))
	    (tc2 (cdr x))))))

