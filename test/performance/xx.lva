(define build-list
  (lambda (n)
    (letrec ((build-it (lambda (n acc) 
			 (if (= n 0) 
			     acc 
			     (build-it (- n 1) (cons n acc))))))
      (build-it n '()))))

(define build-list-aux
  (lambda (n acc) 
    (if (= n 0) 
	acc 
	(build-list-aux (- n 1) (cons n acc)))))

(define build-list2
  (lambda (n)
    (_println 'build-list2)
    (build-list-aux n '())))

(define build-list3
  (lambda (n)
    (letrec ((build-it (lambda (n acc) 
			 (if (= n 0) 
			     acc 
			     (build-it (- n 1) (_si 'cons 'lavaProfile.F n acc))))))
      (build-it n '()))))

