(define build-list
  (lambda (n)
    (letrec ((build-it (lambda (n acc) 
			 (if (= n 0) 
			     acc 
			     (build-it (- n 1) (cons n acc))))))
      (build-it n '()))))

(define build-list-aux
  (lambda (n acc) 
    (if (= n 0) 
	acc 
	(build-list-aux (- n 1) (cons n acc)))))

(define build-list2
  (lambda (n)
    (-println 'build-list2)
    (build-list-aux n '())))

(define build-list3
  (lambda (n)
    (letrec ((build-it (lambda (n acc) 
			 (if (= n 0) 
			     acc 
			     (build-it (- n 1) (-si 'cons 'org.llava.impl.F n acc))))))
      (build-it n '()))))

(begin
  (begin
    (define start -1)
    (define end -1)
    (set! start (java.lang.System.currentTimeMillis))
    (display start)
    (display " ")
    (define l (build-list 10000))
    (display (length l))
    (display " ")
    (equal? l l)
    (set! end (java.lang.System.currentTimeMillis))
    (display end)
    (display " ")
    (display (- end start))
    (newline)
    )
