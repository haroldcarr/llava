(import org.llava.lib.test.Test)
(import org.llava.lib.String)

(begin-test 'strings/list2string-with-space)


(check "list->string-with-space"
       "a 34 34.5 s c"
       (list->string-with-space '(a 34 34.5 "s" #\c)))

(end-test 'strings/list2string-with-space)

