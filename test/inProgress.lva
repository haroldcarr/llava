(define l
  '(
    "llava/tt.lva" 
    "tt.lva"
    "PrimTT.lva"
    ))

(define (gr l)
  (map (lambda (x)
	 (list
	  (getResourceAsStream 
	   (-si 'forName 'java.lang.Class "org.llava.impl.Llava") x)
	  (getResourceAsStream 
	   (-si 'forName 'java.lang.Class "org.llava.impl.runtime.procedure.primitive.llava.PrimAppend") x)))
       l))

(define (lc l)
  (map (lambda (x)
	 (try 
	  (-si 'forName 'java.lang.Class x)
	  (catch (java.lang.Exception e) '())))
       l))

(gr l)
(lc l)
(-si 'getProperties 'java.lang.System)
(-si 'getProperty 'java.lang.System "java.class.path")


