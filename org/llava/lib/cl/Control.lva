(require 'cl/symbols/gensym)

;; TODO - missing result

(define-syntax dotimes
  (lambda (iAndStop . body)
    (let ((gstop (gensym)))
      `(do ((,(car iAndStop) 0 (+ ,(car iAndStop) 1))
	    (,gstop ,(cadr iAndStop)))
	   ((>= ,(car iAndStop) ,gstop))
	 ,@body))))

(provide 'cl/control/dotimes)

