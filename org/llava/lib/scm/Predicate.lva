(package scm Predicates)

(define (boolean? x)
  (or (eq? x true)
      (eq? x false)))

(define (char? x)
  (instanceof x 'java.lang.Character))

(define (procedure? x)
  (instanceof x 'lava.lang.types.Procedure))

(define (string? x)
  (instanceof x 'java.lang.String))

