(require 'lava/lists/build-list)

(define string->list
  (lambda (v)
    (let ((len (length v))
	  (i 0))
      (build-list (lambda () (>= i len))
		  (lambda () 
		    (let ((el (charAt v i)))
		      (set! i (+ i 1))
		      el))))))

(provide 'scm/strings/string->list)

