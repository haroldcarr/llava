(define (memq this that)
  (cond ((null? that) false)
	((eq? this (car that)) that)
	(else (member this (cdr that)))))

(define (member this that)
  (cond ((null? that) false)
	((equal? this (car that)) that)
	(else (member this (cdr that)))))

